#include <stdio.h>
main() {

int nums[] = {2, 7, 11, 15};
int numsSize = 4
int target = 9;

int* twoSum(int* nums, int numsSize, int target) {
    for (int i =0; i < numsSize; i++){
        for (int j = i+1; j < numsSize; j++){
            if (*(nums+i)+*(nums+j)== target){
                int result[] = {i,j};
                return result;        
            }
        }
    }
}

result = twoSum(nums,numsSize,target)
printf(target)

}
